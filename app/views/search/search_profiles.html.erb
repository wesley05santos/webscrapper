<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">URL</th>
    </tr>
  </thead>
  <tbody>
    <% @profiles.each.with_index do |profile, index| %>
      <tr>
        <th scope="row"><%= index + 1 %></th>
        <td><%= profile.name %></td>
        <td><%= profile.url %></td>
      </tr>
    <% end %>
  </tbody>
</table>
